// Example program
#include <iostream>
#include <string>
using namespace std;
int main()
{
 
  int n,t;
  cin>>t;
  while(t--)
  {
       int *a=NULL;
      cin>>n;
    a=new int[n];
    int c;
    cin>>c;
    for (int i=0;i<n;i++)
    cin>>a[i];
    int total=0;bool flag=false;
     for (int i=0;i<n;i++)
     {
        
     total+=a[i];
     
     }
    if(total>c)
     {
      cout<<"No"<<endl;
     }
     else
     {
         cout<<"Yes"<<endl;
         }
    
    
    
      
      
      
      }
  
 
}
